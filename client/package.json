    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.10",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.5",
    "react-router-bootstrap": "^0.23.3",
    "react-router-redux": "^4.0.8",
